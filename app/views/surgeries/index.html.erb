<h1>All scheduled surgeries</h1>

<% @surgeries.each do |surgery| %>
<section id="surgery-<%= surgery.id%>">
  <p>Surgery being performed: <%= link_to surgery.title, "/surgeries/#{surgery.id}" %></p>
  <% surgery.doctors.each do |doctor| %>
    <li>Doctors Performing Surgery: <%= doctor.name %></li>
    <% end %>
</section>
<% end %>
